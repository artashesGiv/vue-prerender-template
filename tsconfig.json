{
  "compilerOptions": {
    "target": "es6",
    "module": "es2020",
    "lib": [
      "ES2020",
      "ES2016",
      "ES2015",
      "dom"
    ],
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "strict": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "importHelpers": true,
    "baseUrl": "./",
    "strictPropertyInitialization": false,
    "paths": {
      "@/*": ["resources/*"]
    }
  },
  "exclude": ["*.js",],
}
